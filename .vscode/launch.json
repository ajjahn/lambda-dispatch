  "version": "0.2.0",
  "configurations": [
    {
      "name": "LambdaLB bootstrap (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/PwrDrvr.LambdaDispatch.LambdaLB/bin/Debug/net8.0/bootstrap",
      "args": [],
      "cwd": "${workspaceFolder}/src/PwrDrvr.LambdaDispatch.LambdaLB",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": ".NET Lambda Test Tool",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "<home-directory>/.dotnet/tools/dotnet-lambda-test-tool-8.0",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "LambdaLB (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/test/PwrDrvr.LambdaDispatch.LambdaLB.Tests/bin/Debug/net8.0/PwrDrvr.LambdaDispatch.LambdaLB.Tests.dll",
      "args": [],
      "cwd": "${workspaceFolder}/test/PwrDrvr.LambdaDispatch.LambdaLB.Tests",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Router (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/PwrDrvr.LambdaDispatch.Router/bin/Debug/net8.0/PwrDrvr.LambdaDispatch.Router.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/PwrDrvr.LambdaDispatch.Router",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "AWS_ACCESS_KEY_ID": "test-access-key-id",
        "AWS_SECRET_ACCESS_KEY": "test-secret-access-key",
        "AWS_SESSION_TOKEN": "test-session-token"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}